{
  "name": "hydrator",
  "version": "0.0.1",
  "description": "Turn Twitter identifiers into JSON",
  "repository": "https://github/com/hydrator",
  "scripts": {
    "clean": "rimraf lib static",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack --config webpack.prod.config.js src/index.js",
    "build": "npm run build:lib && npm run build:umd",
    "start": "node server.js",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint-plugin-react": "^3.6.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.6",
    "redux-thunk": "^1.0.3"
  }
}
